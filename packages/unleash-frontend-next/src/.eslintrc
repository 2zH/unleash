{
    "extends": [
        "finn",
        "finn-react"
    ],
    "env": {
        "browser": true,
        "commonjs": true,
	    "es6": true
    },
    "rules": {
        "react/sort-comp": "off"
    },
    "ecmaFeatures": {
        "sourceType": "module"
    },
    "parserOptions": {
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "experimentalObjectRestSpread": true
        }
    }
}
